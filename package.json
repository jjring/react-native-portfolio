  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "android:update": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:release": "react-native run-android --variant release",
    "ios": "react-native run-ios",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "web": "react-scripts start",
    "web-bundle": "react-scripts build",
    "predeploy": "npm run web-bundle",
    "deploy": "gh-pages -d build",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'"
